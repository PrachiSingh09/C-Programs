            cout<<i;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}

V. /* ^^^^^
            ^^^^
              ^^^
                ^^
                  ^*/
#include <iostream>
using namespace std;

int main()
{
    int n,i=1;
    cin>>n;
    while(i<=n){
        int space=1;
        int j=1;
        //Printing space
        while(space<i){
            cout<<" ";
            space=space+1;
        }
        //Printing stars
        while(j<=(n-i+1)){
            cout<<"*";
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}

