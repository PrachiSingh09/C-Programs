#include <iostream>
using namespace std;

int main()
{
    int n,i=1;
    cin>>n;
    while(i<=n){
        int space=(n-i);
        int j=1;
        //Printing space
        while(space){
            cout<<" ";
            space=space-1;
        }
        //Printing stars
