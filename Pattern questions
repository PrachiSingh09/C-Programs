    while(i<=n){
        int j=1;
        while(j<=i){
            cout<<ch;
            ch+=1;
            j++;
        } 
        cout<<endl;
        i++;
    }
    return 0;
}

S. /*1
        2 3
        3 4 5 */
METHOD:1
#include <iostream>
using namespace std;

int main()
{
    int n,i=1;
    cin>>n;
    while(i<=n){
        int value=i;
        int j=1;
        while(j<=i){
            cout<<value;
            value+=1;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}

METHOD:2
#include <iostream>
using namespace std;

int main()
{
    int n,i=1;
    cin>>n;
    while(i<=n){
        int j=i;
        while(j<i*2){
            cout<<j;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}

T. /*       ^
              ^^
            ^^^
         ^^^^*/
#include <iostream>
using namespace std;

int main()
{
    int n,i=1;
    cin>>n;
    while(i<=n){
        int space=(n-i);
        int j=1;
        //Printing space
        while(space){
            cout<<" ";
            space=space-1;
        }
        //Printing stars
        while(j<=i){
            cout<<"^";
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}

