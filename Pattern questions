        int space=1;
        int j=1;
        //Printing space
        while(space<i){
            cout<<" ";
            space=space+1;
        }
        //Printing stars
        while(j<=(n-i+1)){
            cout<<i;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}

X. /* 1 2 3 4
            2 3 4
               3 4
                  4*/
#include <iostream>
using namespace std;

int main()
{
    int n,i=1;
    cin>>n;
    while(i<=n){
        int value=i;
        int space=1;
        int j=1;
        //Printing space
        while(space<i){
            cout<<" ";
            space=space+1;
        }
        //Printing stars
        while(j<=(n-i+1)){
            cout<<value;
            value++;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}

Y. /*        1
             23
           456
         78910*/
#include <iostream>
using namespace std;

int main()
{
    int n,i=1,val=1;
    cin>>n;
    while(i<=n){
        int space=1;
        int j=1;
        //Printing space
        while(space<=(n-i)){
            cout<<"  "; // notice two spaces.
            space=space+1;
        }
        //Printing stars
        while(j<=i){
            cout<<val<<" ";
            val++;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}

Z. /* ^^^^
         ^^^
         ^^
         ^*/
using namespace std;

int main()
{
    int n,i=1;
    cin>>n;
    while(i<=n){
        int j=1;
        while(j<=n-i+1){
            cout<<"*";
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}
