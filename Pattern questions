H./*1 2 3
        4 5 6
        7 8 9*/
#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int i=1,count=1;
    while(i<=n){
        int j=1;
        while(j<=n){
            cout<<count<<" ";
            count=count+1;
            j++;
        }
        cout<<endl;
        i++;
    }

    return 0;
}

