K. /*ABC
        ABC
         ABC*/
METHOD1:
#include <iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int i=1;
    while(i<=n){
        int j=1;
        char ch='A';
        while(j<=n){
            cout<<ch;
            ch=ch+1;
            j++;
        } 
        cout<<endl;
        i++;
    }

    return 0;
}

METHOD2:
#include <iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int i=1;
    while(i<=n){
        int j=1;
        char ch='A'+j-1;
        while(j<=n){
            cout<<ch;
            ch=ch+1;
            j++;
        } 
        cout<<endl;
        i++;
    }

    return 0;
}

