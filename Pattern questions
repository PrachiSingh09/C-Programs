P. /*A
        BC
        CDE
         DEFG*/
#include <iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int i=1,j=1;
    while(i<=n){
        int j=1;
        char ch='A'+i+j-2;
        while(j<=i){
            cout<<ch;
            ch=ch+1;
            j++;
        } 
        cout<<endl;
        i++;
    }

    return 0;
}

