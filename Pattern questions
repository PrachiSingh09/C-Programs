A. //4X4 star pattern
#include <iostream>
using namespace std;
//Operators
int main() {
  int n;
  cin>>n;
  int i=1;
  while(i<=n){
   int j=1;
    while(j<=n){
      cout<<"*";
    }
    cout<<endl;
    i=i+1;
  }
    return 0;
}

B. 
/*1111
  2222
3333
4444*/
#include <iostream>
using namespace std;

int main() {
  int n;
  cin>>n;
  int i=1;
  
  while(i<=n){
   int j=1;
   
    while(j<=n){
      cout<<i;
      j=j+1;
    }
    
    cout<<endl;
    i=i+1;
  }
    return 0;
}

